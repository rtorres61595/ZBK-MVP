{
  "name": "zbk-mvp",
  "version": "1.1.0",
  "description": "Official Repo for ZeusBeKind's MVP Created by CERNT.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlgoRen/ZBK-MVP.git"
  },
  "author": "Ren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlgoRen/ZBK-MVP/issues"
  },
  "homepage": "https://github.com/AlgoRen/ZBK-MVP#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
